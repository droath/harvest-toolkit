{
    "name": "droath/harvest-toolkit",
    "description": "Interact with the Harvest time tracking service from a CLI.",
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "bin": [
        "bin/ht"
    ],
    "authors": [
        {
            "name": "Travis Tomka",
            "email": "underoathosx@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "symfony/console": "^3.0|^4.0|^5.0",
        "wearerequired/harvest-api-php-client": "^0.1.1",
        "php-http/guzzle6-adapter": "^2.0",
        "symfony/dependency-injection": "^3.0|^4.0|^5.0",
        "symfony/yaml": "^3.0|^4.0|^5.0",
        "symfony/cache": "^3.0|^4.0|^5.0"
    },
    "require-dev": {
        "phpmd/phpmd": "^2.8",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Droath\\HarvestToolkit\\": "./src"
        }
    }
}
